INPUT, SELECT, TEXTAREA
{
   font-size		: 10pt;
   font-family      : georgia,tahoma;
   border-width     : 1px;
   border-style     : solid;
   border-color     : #999999;
}

INPUT.submit
{
  background-color: gainsboro;
}

TABLE.data { 
	background-color: white;
	border: 0px;
}

TABLE.data th { 
	border-bottom: 1px solid black;
	padding: 3px;
}

TABLE.data tfoot td { 
	border-bottom: 1px solid black;
}

TABLE.data tr:hover { 
	background-color: gainsboro;
	color: inherit;
}

TABLE.data tbody td { 
	padding: 3px 5px 3px 5px;
	border-bottom: 1px solid gainsboro;

}

TABLE.data thead tr:hover { 
	background-color: transparent;
	color: inherit;
}


TABLE.dialog {
	border-width: 1px;
	border-spacing: 0px;
	padding: 2px 2px 2px 2px;
	border-style: outset;
	border-color: #336699;
	border-collapse: collapse;
	background-color: #cccccc;
}	

TABLE.dialog th {
	border-width: 0px;
	padding: 4px 4px 4px 4px;
	border-style: inset;
	border-color: gray;
	background-color: #336699;
	color: white;
}

TABLE.dialog th a {
	color: gainsboro;
	font-weight: normal;
}

TABLE.dialog th a:visited {
	color: gainsboro;
	font-weight: normal;
}

dialog th a {
	color: gainsboro;
	font-weight: normal;
}

dialog th a:visited {
	color: gainsboro;
	font-weight: normal;
}


TABLE.dialog td { 
	border-width: 0px;
	padding: 5px 5px 5px 5px;
	border-style: inset;
	border-color: gray;
	background-color: #cccccc;
}

DIV.dialog { 
	visibility: hidden; 
	position: absolute; 
	left: 10%; 
	top: 90%; 
	zIndex: 100;
}

.closeDialog {
	text-align: right;
	border-width: 0px 0px 0px 0px;
}

IMG.dialog { 
	padding: 2px 2px 2px 2px;
	border-width: 0px;
}

DIV.page { 
	text-align: right;
	padding: 4px;
}

TR.inactive { 
	font-style: italic;
	text-decoration: line-through;
}

.searchBar {
	border-width: 1px 0px 0px 1px;
	border-style: solid;
	border-spacing: 1px;
	border-color: #336699;
	padding: 5px; 
	background-color: gainsboro;
	color: black; font-family:   georgia,tahoma; font-size: 10pt;
}	

.userBar {
