<%@ taglib uri="http://struts.apache.org/tags-tiles" prefix="tiles" %>
<%@ taglib uri="/ndg-message" prefix="message" %>
<html>
<tiles:insert attribute="head" />
	<!--background="/CRM/images/background_dark.gif"--> 
<body align="center" style="background-repeat: repeat-x; background-color: #CCCC99 ; ">
<tiles:insert attribute="title" />
<tiles:insert attribute="tabs" />
<table border="0" cellspacing="0" cellpadding="0" width="902" align="center">
	<tr>
		<td width="1" class="colorMain"></td>
		<td width="900" valign="top" align="center" bgcolor="white">
			<table border="0" cellspacing="0" cellpadding="0" width="800">
				<tr height="5"><td></td></tr>
				<tr>
					<td align="left">
						<table border="0" cellspacing="0" cellpadding="0" width="800">
							<tr>
								<td width="400" class="textH2"><tiles:insert attribute="page_title" /></td>
								<td width="400" valign="top" align="right">
									<table border="0">
										<tr height="8"><td></td></tr>
										<tr><td><tiles:insert attribute="page_options" /></td></tr>
									</table>
								</td>
							</tr>
							<tr height="10"><td></td></tr>
							<tr>
								<td colspan="2">
									<message:messages />								
								</td>
							</tr>
						</table>
						<tiles:insert attribute="body" />
						<br>
						<br>
					</td>
				</tr>
			</table>
		</td>
		<td width="1" class="colorMain"></td>
	</tr>
	<tr height="1">
		<td colspan="3" class="colorMain"></td>
	</tr>
</table>
<tiles:insert attribute="bottom" />
<tiles:insert attribute="dialogBoxes" />
</body>
</html>
