	function validateFireExtinguisher()
	{
		if(isEmpty(frmExtinguisher.year, 'Year', 'true')
		|| isEmpty(frmExtinguisher.manufacturerId, 'Manufacturer', 'true')
		|| isEmpty(frmExtinguisher.weightId, 'Weight', 'true')
		|| isEmpty(frmExtinguisher.chemicalId, 'Chemical', 'true')
		|| !ValNumber(frmExtinguisher.year, 'Year', true, 'false')
		|| !ValNumber(frmExtinguisher.floor, 'Floor', true)) {
			return false;
		}
		return true;
		
	}