<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN" "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
<tlibversion>1.2</tlibversion>
<jspversion>1.1</jspversion>
<shortname>report</shortname>
<uri>ndg-report</uri>    
<tag>
    	<name>sort</name>
    	<tagclass>com.ndg.report.SortTag</tagclass>
    	<info>Displays the paging numbers for the Report Framework</info>
    	<attribute>
    		<name>reportKey</name>
    		<required>true</required>
    	</attribute>
    	<attribute>
    		<name>pageActionHref</name>
    		<required>true</required>
    	</attribute>
		<attribute>
			<name>style</name>
		</attribute>
		<attribute>
			<name>suppressLabel</name>
		</attribute>
		<attribute>
			<name>groupAttribute</name>
			<required>false</required>
		</attribute>
    </tag>    
    <tag>
    	<name>sortattribute</name>
    	<tagclass>com.ndg.report.SortAttributeTag</tagclass>
    	<info>an attribute that may be sorted on</info>
    	<attribute>
    		<name>name</name>
    		<required>true</required>
    	</attribute>
    	<attribute>
    		<name>value</name>
    		<required>true</required>
    	</attribute>
    </tag>
    <tag>
    	<name>filter</name>
    	<tagclass>com.ndg.report.FilterTag</tagclass>
    	<info>A tag to print the html for filtering a report</info>
    	<attribute>
    		<name>name</name>
    		<required>false</required>
    	</attribute>
    	<attribute>
    		<name>attributeName</name>
    		<required>false</required>
    	</attribute>
    	<attribute>
    		<name>reportKey</name>
    		<required>true</required>
    	</attribute>
    	<attribute>
    		<name>pageActionHref</name>
    		<required>true</required>
    	</attribute>
		<attribute>
			<name>style</name>
		</attribute>
		<attribute>
			<name>suppressFindAllButton</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>size</name>
			<required>false</required>
		</attribute>
		<attribute>
			<name>typable</name>
			<required>false</required>
		</attribute>
	</tag>    	
    <tag>
    	<name>filterattribute</name>
    	<tagclass>com.ndg.report.FilterAttributeTag</tagclass>
    	<info>an attribute that may be filtered on</info>
    	<attribute>
    		<name>name</name>
    		<required>true</required>
    	</attribute>
    	<attribute>
    		<name>value</name>
    		<required>false</required>
    	</attribute>
    	<attribute>
    		<name>attributeName</name>
    		<required>true</required>
    	</attribute>
    </tag>	
    <tag>
    	<name>page</name>
    	<tagclass>com.ndg.report.PageTag</tagclass>
    	<info>Displays the paging numbers for the Report Framework</info>
    	<attribute>
    		<name>reportKey</name>
    		<required>true</required>
    	</attribute>
    	<attribute>
    		<name>pageActionHref</name>
    		<required>true</required>
    	</attribute>
    </tag>
</taglib>



	